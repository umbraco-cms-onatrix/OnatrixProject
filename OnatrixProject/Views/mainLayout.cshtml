@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
	var pageTitle = !string.IsNullOrEmpty(ViewBag.Title) ? $"{ViewBag.Title} | Onatrix- Financial Solucions For Your Business" : "Onatrix- Financial Solucions For Your Business";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link rel="stylesheet" href="https://kit.fontawesome.com/a576612222.css" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/theme.css">
    <link rel="stylesheet" href="/css/main.css">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <title>@pageTitle</title>
</head>
<body>
    <div class="wrapper">
        <header>
            <section class="social-header bg-primary" >
                <div class="container">
                    <div class="social">
                        <h6>Klarabergsviadukten 90, 111 64 Stockholm</h6>
                        <div class="social-media">
                            <div class="vl"></div>
                            <a href="#" class="facebook-icon"><i class="fa-brands fa-facebook-f"></i></a>
                            <div class="vl"></div>
                            <a href="#" class="tx-icon"><i class="fa-brands fa-x-twitter"></i></a>
                            <div class="vl"></div>
                            <a href="#" class="link-icon"><i class="fa-brands fa-linkedin-in"></i></a>
                            <div class="vl"></div>
                            <button href="#" class="btn-menu"><i class="fa-solid fa-bars"></i></button>
                        </div>

                    </div>

                </div>

            </section>
            <section class="nav-bar bg-light">
                <div class="container ">
                    <nav class="nav-contact">
                        <img src="/Images/Icon/Brand.svg" alt="Brand logotype">
                        <div class="topnav">
                            <a href="/about">About</a>
                            <a href="/services">Services</a>
                            <a href="/projects">Projects</a>
                            <a href="/contact">Contact</a>
                        </div>
                        <a href="" class="search-btn"><i class="fa-solid fa-magnifying-glass"></i></a>
        
                    </nav>
        
                </div>
            

            </section>


        </header>

        <main>
        @RenderBody()

        </main>

        <footer>
            <section class="footer bg-dark">
                <div class="container">
                    <p>&copy; Onatrix. All Rights Reserved. Credits to</p> <span id="footer-highlight">Themesflat</span> 
                </div>

            </section>

        </footer>

    </div>
    
    <script src="/scripts/main.js"></script>
    <script src="https://kit.fontawesome.com/3fa4ea9af4.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
</body>
</html>